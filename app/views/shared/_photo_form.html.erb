<%= form_for(@photo) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <div class="field">
    <%= f.text_field :caption, placeholder: "Compose a caption" %>
  </div>
  <div class="field">
  	<%= f.label :image %><br />
  	<%= f.file_field :image %>
  </div>
  <%= f.submit "Upload", class: "btn btn-large btn-primary" %>
<% end %>